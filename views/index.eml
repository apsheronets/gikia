% open Lwt
% open Utils
% let f a buf =
  <% Header_table.f a buf >|= fun () -> %>
  <h1><%= a#title %></h1>
  <% match a#lis with
     | [] -> %>
    <p>This directory is empty.</p>
  <% | lis -> %>
    <ul>
    <% lis >> List.iter (fun (_, path, title) -> %>
      <li><%=raw link_to path title %></li>
    <% ); %>
    </ul>
